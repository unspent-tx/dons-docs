{
  "name": "aiken-autocomplete",
  "displayName": "Aiken Autocomplete",
  "description": "Autocomplete for Aiken functions with auto-import statements",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:aiken"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "aiken",
        "extensions": [
          ".ak"
        ],
        "aliases": [
          "Aiken"
        ]
      }
    ],
    "configuration": {
      "title": "Aiken Autocomplete",
      "properties": {
        "aikenAutocomplete.enableAutoImport": {
          "type": "boolean",
          "default": true,
          "description": "Enable automatic import statements for Aiken functions"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}
